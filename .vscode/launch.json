    "version": "0.2.0",
    "configurations": [
        {
            "name": "example (Flutter Windows)",
            "program": "lib/main.dart",
            "cwd": "example",
            "deviceId": "windows",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "Launch example.exe (MSVC)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/example/build/windows/x64/runner/Debug/example.exe",
            "cwd": "${workspaceFolder}/example/build/windows/x64/runner/Debug",
            "stopAtEntry": false,
            "console": "integratedTerminal",
            "symbolSearchPath": "${workspaceFolder}/example/build/windows/x64/plugins/microsoft_store_upgrader/Debug;${workspaceFolder}/example/build/windows/x64/flutter/Debug"
        }
    ]
}